pipeline{
  agent any

  stages {
    stage('Checkout') {
      steps {
        git branch: 'main', url: 'https://github.com/BKarthik03/Devops_Exam'
      }
    }

    stage('Build') {
      steps {
        echo "Building..."
      }
    }

    stage('Test') {
      steps {
        echo "Testing..."
      }
    }
  }
}
  
